	<h1>함수의 호이스팅</h1>
	<script type="text/javascript">
		myfunc(70);
		var myfunc = function test(num){
			if(num>=90){
				console.log("통과");
			}else{
				console.log("다시작업");
			}
		}
	myfunc(100);
	
	</script>
