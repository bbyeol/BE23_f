<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>Insert title here</title>
</head>
<body>
	<script type="text/javascript">
		//증감연산자
		let x;
		let y;
		
		x=5;
		y=x++;
		document.wrtie("y=x++ => x=" +x+",y="+y+"<br/>");
		
		x=5;
		y=++x;
		document.wrtie("y=++x => x=" +x+",y="+y+"<br/>");
		
		x=5;
		y=x--;
		document.wrtie("y=x-- => x=" +x+",y="+y+"<br/>");
		
		x=5;
		y=--x;
		document.wrtie("y=--x => x=" +x+",y="+y+"<br/>");
		
		//비교연산자는 결과가 boolean 형식->조건을 판단해서 처리해야 하는 작업에 사용
		//비교연산자는 true/false
		let num1=100;
		let num2=200;
		let num3=100;
		let num4="100"; //문자열
		
		document.write("num1==num3:"+(num1==num3)+"<br/>");		
		document.write("num1===num3:"+(num1===num3)+"<br/>");
		
		document.write("num1==num4:"+(num1==num4)+"<br/>");		
		document.write("num1===num4:"+(num1===num4)+"<br/>");
		
	</script>

</body>
</html>